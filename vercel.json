{
    "version": 2,
    "builds": [
      {
        "src": "backend/src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/next"
      }
    ],
    "scripts": {
        "build": "npm run build && ls -l frontend/dist"
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/.next/$1"
      }
    ]
  }
  
